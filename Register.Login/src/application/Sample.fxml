<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="500.0" prefWidth="416.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.SampleController">
	<children>
		<GridPane hgap="10.0" vgap="20.0">
			<columnConstraints>
				<ColumnConstraints halignment="RIGHT"
					hgrow="SOMETIMES" maxWidth="132.0" minWidth="10.0" prefWidth="84.0" />
				<ColumnConstraints hgrow="SOMETIMES"
					maxWidth="287.0" minWidth="204.0" prefWidth="267.0" />
				<ColumnConstraints hgrow="SOMETIMES"
					maxWidth="200.0" minWidth="0.0" prefWidth="5.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="0.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="0.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="0.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="0.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="9.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="4.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="0.0" vgrow="SOMETIMES" />
				<RowConstraints minHeight="0.0" vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<TextField fx:id="firstNameText" maxHeight="30.0"
					maxWidth="200.0" onKeyReleased="#isEmpty" onKeyTyped="#fNameKey"
					prefHeight="30.0" prefWidth="200.0" GridPane.columnIndex="1" />
				<Label fx:id="firstName" text="First Name:" />
				<Label fx:id="lastName" text="Last Name:"
					GridPane.rowIndex="1" />
				<Label fx:id="username" text="Username:" GridPane.rowIndex="2" />
				<Label fx:id="password" text="Password:" GridPane.rowIndex="3" />
				<Label fx:id="retypePass" text="Retype Pass:"
					GridPane.rowIndex="4" />
				<Label fx:id="birthDate" text="BirthDate:"
					GridPane.rowIndex="5" />
				<Label fx:id="adress" prefHeight="21.0" prefWidth="59.0"
					text=" Adress:" GridPane.rowIndex="6" />
				<TextField fx:id="lastNameText" maxHeight="30.0"
					maxWidth="200.0" onKeyReleased="#isEmpty" onKeyTyped="#lNameKey"
					GridPane.columnIndex="1" GridPane.rowIndex="1" />
				<TextField fx:id="userText" maxHeight="30.0"
					maxWidth="200.0" onKeyReleased="#isEmpty" onKeyTyped="#userKey"
					GridPane.columnIndex="1" GridPane.rowIndex="2" />
				<TextField fx:id="passText" maxHeight="30.0"
					maxWidth="200.0" onKeyReleased="#isEmpty" onKeyTyped="#passKey"
					prefHeight="30.0" prefWidth="200.0" GridPane.columnIndex="1"
					GridPane.rowIndex="3" />
				<TextField fx:id="retypePassText" maxHeight="30.0"
					maxWidth="200.0" onKeyReleased="#isEmpty" onKeyTyped="#rPassKey"
					GridPane.columnIndex="1" GridPane.rowIndex="4" />
				<DatePicker fx:id="birthDateText" editable="false"
					maxHeight="30.0" maxWidth="200.0" onKeyReleased="#isEmpty"
					GridPane.columnIndex="1" GridPane.rowIndex="5" />
				<TextArea fx:id="adressText" maxHeight="100.0"
					maxWidth="250.0" minHeight="100.0" minWidth="250.0"
					onKeyReleased="#isEmpty" onKeyTyped="#aKey" prefHeight="100.0"
					prefWidth="250.0" wrapText="true" GridPane.columnIndex="1"
					GridPane.rowIndex="6" />
				<Button fx:id="registerButton" mnemonicParsing="false"
					onAction="#button" prefHeight="31.0" prefWidth="102.0"
					text="Register" GridPane.columnIndex="1" GridPane.rowIndex="7" />
				<Button mnemonicParsing="false" onAction="#changeScreen"
					prefHeight="31.0" prefWidth="68.0" text="Login"
					GridPane.rowIndex="7" />
			</children>
			<padding>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</padding>
		</GridPane>
		<Text fx:id="tText" fill="#a80000" layoutX="215.0"
			layoutY="460.0" strokeType="OUTSIDE" strokeWidth="0.0"
			wrappingWidth="193.0" />
	</children>
</AnchorPane>
